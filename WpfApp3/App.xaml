<Application 
    x:Class="WpfApp3.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:WpfApp3"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/WpfApp3;Component/Resources/ControlDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
            
            <FontFamily x:Key="Pretendard_Light">/WpfApp3;component/Font/#Pretendard Light</FontFamily>
            <FontFamily x:Key="Pretendard_Medium">/WpfApp3;component/Font/#Pretendard Medium</FontFamily>
            <FontFamily x:Key="Pretendard_Bold">/WpfApp3;component/Font/#Pretendard Bold</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
